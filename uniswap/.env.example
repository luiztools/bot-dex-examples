# Your Moralis API KEY
MORALIS_API_KEY=

# The interval time in ms to monitor. Ex: 300000 (5min)
INTERVAL=

# The ERC-20 base token address at blockchain. Ex: 0x1f9840a85d5aF5bf1D1762F925BDADdC4201F984 (UNI - Goerli)
TOKEN0_ADDRESS=

# The ERC-20 quote token address at blockchain. Ex: 0xB4FBF271143F4FBf7B91A5ded31805e42b2208d6 (WETH - Goerli)
TOKEN1_ADDRESS=

# Your wallet address on blockchain
WALLET=

# Your wallet private key
PRIVATE_KEY=

# Your blockchain name, lowercase. Ex: goerli
NETWORK=

# Your Infura node API Key
INFURA_API_KEY=

# The Uniswap V3 router contract address. Ex: 0x68b3465833fb72A70ecDF485E0e4C7bD8665Fc45 (Goerli)
ROUTER_ADDRESS=

# The coin price (in USD) which will trigger the "buy" swap. Ex: 5
PRICE_TO_BUY=

# The coin amount (in ether) for each "buy" swap. Ex: 0.1
AMOUNT_TO_BUY=

# The desired profitability multiplier which will trigger the "sell" swap. Ex: 1.1 (10%)
PROFITABILITY=